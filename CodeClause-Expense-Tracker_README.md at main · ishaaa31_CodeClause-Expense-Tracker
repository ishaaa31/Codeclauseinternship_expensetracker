  [popover]:not(.\:popover-open, dialog[open]) { display: revert; }
  [anchor]:is() { inset: auto; }
}

@supports selector([popover]:popover-open) {
  [popover]:not(.\:popover-open, dialog[open]) { display: revert; }
  [anchor]:is() { inset: auto; }
}

